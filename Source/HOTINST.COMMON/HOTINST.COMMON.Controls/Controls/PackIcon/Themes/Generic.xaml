<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:packIcon="clr-namespace:HOTINST.COMMON.Controls.Controls.PackIcon">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/HOTINST.COMMON.Controls;component/Controls/PackIcon/Themes/IconPacks.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type packIcon:PackIconMaterial}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconMaterial}" />
    <Style TargetType="{x:Type packIcon:PackIconMaterialLight}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconMaterialLight}" />
    <Style TargetType="{x:Type packIcon:PackIconFontAwesome}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconFontAwesome}" />
    <Style TargetType="{x:Type packIcon:PackIconOcticons}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconOcticons}" />
    <Style TargetType="{x:Type packIcon:PackIconModern}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconModern}" />
    <Style TargetType="{x:Type packIcon:PackIconEntypo}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconEntypo}" />
    <Style TargetType="{x:Type packIcon:PackIconSimpleIcons}" BasedOn="{StaticResource MahApps.Metro.Styles.PackIconSimpleIcons}" />
</ResourceDictionary>